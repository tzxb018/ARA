<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="false" >
    <shape>
      <solid
          android:color="#7c7c7c" />
      <stroke
          android:width="12dp"
          android:color="#00FFFFFF" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="17dp"
          android:top="10dp"
          android:right="17dp"
          android:bottom="10dp" />
    </shape>
  </item> 
</selector>
